                    "type": "integer",
                    "default": 0,
                    "minimum": 0,
                    "maximum": 15
                },
                "lightLevelBlue": {
                    "type": "integer",
                    "default": 0,
                    "minimum": 0,
                    "maximum": 15
                },
                "rotXZ": {
                    "type": "integer",
                    "default": 0
                },
                "rotation": {
                    "type": "array",
                    "items": {
                        "type": "number"
                    },
                    "minItems": 3,
                    "maxItems": 3
                },
                "dropId": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "dropParams": {
                    "type": "object",
                    "properties": {
                        "blockstate": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": true
                },
                "intProperties": {
                    "type": "object",
                    "properties": {
                        "fuelTicks": {
                            "type": "integer",
