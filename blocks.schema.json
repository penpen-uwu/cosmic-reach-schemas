{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "blocks.schema.json",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string"
        },
        "stringId": {
            "type": "string"
        },
        "blockEntityId": {
            "type": "string"
        },
        "blockEntityParams": {
            "type": "object",
            "properties": {
                "numSlots": {
                    "type": "integer"
                }
            },
            "additionalProperties": false
        },
        "defaultProperties": {
            "type": "object",
            "properties": {
                "modelName": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "langKey": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "swapGroupId": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "allowSwapping": {
                    "type": "boolean",
                    "default": true
                },
                "blockEventsId": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "itemIcon": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "hardness": {
                    "type": "number",
                    "default": 1.5
                },
                "blastResistance": {
                    "type": "number",
                    "default": 100.0
                },
