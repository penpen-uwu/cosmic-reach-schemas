                "bounciness": {
                    "type": "number",
                    "default": 0.0
                },
                "refractiveIndex": {
                    "type": "number",
                    "default": 1.0
                },
                "placementRules": {
                    "type": "string",
                    "default": ""
                }
            },
            "additionalProperties": false
