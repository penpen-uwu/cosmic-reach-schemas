                                "type": "string"
                            }
                        },
                        "isOpaque": {
                            "type": "boolean",
                            "default": true
                        },
                        "stopsLasers": {
                            "type": "boolean",
                            "default": true
                        },
                        "walkThrough": {
                            "type": "boolean",
                            "default": false
                        },
                        "lightAttenuation": {
                            "type": "integer",
                            "default": 15,
                            "minimum": 0,
                            "maximum": 15
                        },
                        "canRaycastForBreak": {
                            "type": "boolean",
                            "default": true
                        },
                        "canRaycastForPlaceOn": {
                            "type": "boolean",
                            "default": true
                        },
                        "canRaycastForReplace": {
                            "type": "boolean",
                            "default": false
                        },
                        "canDrop": {
                            "type": "boolean",
                            "default": true
                        },
                        "isFluid": {
                            "type": "boolean",
